{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "tooling": {
    "defaultNamespace": "App.API"
  },

  "dependencies": {
    "Microsoft.AspNet.IISPlatformHandler": "1.0.0-rc1",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1",
    "Microsoft.AspNet.StaticFiles": "1.0.0-rc1",
    "Microsoft.Framework.Configuration.Abstractions": "1.0.0-rc1",
    "Microsoft.Framework.Configuration.Json": "1.0.0-rc1",
    "Microsoft.Framework.OptionsModel": "1.0.0-rc1",
    "Microsoft.Framework.Logging": "1.0.0-rc1",
    "Microsoft.Framework.Logging.Console": "1.0.0-rc1",
    "Microsoft.Framework.Logging.Debug" : "1.0.0-rc1",
    "EntityFramework.SQLite": "7.0.0-rc1",
    "EntityFramework.Commands": "7.0.0-rc1",
    "AspnetWebApi2Helpers.Serialization": "1.0.0-*",
    "AspnetWebApi2Helpers.Serialization.Protobuf": "1.0.0-*",
    "PagedList": "1.17.0",
    "App.Data": "1.0.0-*",
    "App.Models": "1.0.0-*",
    "App.Services": "1.0.0-*" 
  },

  "commands": {
    "kestrel": "Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:9030",
    "web": "Microsoft.AspNet.Server.WebListener --server.urls http://localhost:9030"
  },

  "frameworks": {
    "dnx451": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc"
  ]
}
