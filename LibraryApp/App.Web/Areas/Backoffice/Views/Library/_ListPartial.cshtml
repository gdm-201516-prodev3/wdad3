@model IEnumerable<App.Models.Library>
<div class="panel panel-default">
	<div class="panel-body">
		@if (Model != null && Model.Count() > 0)
		{
			<div class="table-responsive">
				<table class="table table-striped">
					<thead>
						<tr>
							<th>Name</th>
							<th>Code</th>
							<th>Description</th>
							<th>Created</th>
							<th>Actions</th>
						</tr>
					</thead>
					<tbody>
						@foreach (var library in Model)
						{
							<tr>
								<td>@library.Name</td>
								<td>@library.Code</td>
								<td>@library.Description</td>
								<td>@library.CreatedAt</td>
								<td>
									<a href="@Url.Action("Edit", new { id = @library.Id })" title="Edit Mediatheek: @library.Name"><span class="fa fa-edit"></span></a>
									<a href="" title="Delete Mediatheek: @library.Name"><span class="fa fa-trash"></span></a>
									<a href="" title="Softdelete Mediatheek: @library.Name"><span class="fa fa-eye"></span></a>
								</td>
							</tr>
						}
					</tbody>
				</table>
			</div>
		}
	</div>
</div>